{
    "name": "media-alchemyst/media-alchemyst",
    "type": "library",
    "description": "An Object Oriented wrapper for easy multimedia conversion, based on Imagine, FFMpeg, SwfTools, Unoconv and other libs",
    "keywords": ["video processing", "video", "audio processing", "audio", "image", "image processing"],
    "license": "MIT",
    "authors": [
        {
            "name": "Romain Neutron",
            "email": "imprec@gmail.com",
            "homepage": "http://www.lickmychip.com/"
        },
        {
            "name": "Phraseanet Team",
            "email": "info@alchemy.fr",
            "homepage": "http://www.phraseanet.com/"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url":  "https://github.com/romainneutron/Imagine.git"
        }
    ],
    "require": {
        "php"                         : ">=5.3.3",
        "pimple/pimple"               : "~1.0",
        "mediavorus/mediavorus"       : "~0.2",
        "swftools/swftools"           : "~0.1.0",
        "alchemy/ghostscript"         : "~0.2.0",
        "php-ffmpeg/php-ffmpeg"       : "~0.2.0",
        "php-unoconv/php-unoconv"     : "~0.1.0",
        "php-mp4box/php-mp4box"       : "~0.1.0",
        "imagine/imagine"             : "dev-ColorSpaces",
        "monolog/monolog"             : "~1.0",
        "symfony/console"             : "~2.0"
    },
    "require-dev": {
        "phpexiftool/phpexiftool"     : "~0.1",
        "silex/silex"                 : "1.0.*@dev"
    },
    "autoload": {
        "psr-0": {
            "MediaAlchemyst": "src"
        }
    }
}
